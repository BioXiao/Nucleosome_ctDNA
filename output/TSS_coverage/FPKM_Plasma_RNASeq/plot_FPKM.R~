lines(bottom$Position,bottom$Mean.Cov,col=rgb(1,0,0))
polygon(c(top$Position,rev(top$Position)),c(top$LowerBound,rev(top$UpperBound)),col=rgb(0.75,0.2,1,0.2), border = FALSE)
lines(top$Position,top$Mean.Cov,col=rgb(0.75,0.2,1))
polygon(c(top100$Position,rev(top100$Position)),c(top100$LowerBound,rev(top100$UpperBound)),col=rgb(0.2,0.75,0,0.2), border = FALSE)
lines(top100$Position,top100$Mean.Cov,col=rgb(0.2,0.75,0))
polygon(c(low$Position,rev(low$Position)),c(low$LowerBound,rev(low$UpperBound)),col=rgb(0.2,0.75,1,0.2), border = FALSE)
lines(low$Position,low$Mean.Cov,col=rgb(0.2,0.75,1))
