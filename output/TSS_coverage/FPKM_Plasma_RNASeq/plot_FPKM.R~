top<-read.csv("MergedControls_Plasma_FPKM_over8_tss.txt",header=TRUE,sep="\t")
bottom<-read.csv("MergedControls_Plasma_FPKM_between1_8_tss.txt",header=TRUE,sep="\t")
png(filename = paste("MergedControls_Plasma_FPKM_8_vs_1to8_tss.png",sep=""), width = 1180, height = 1180, units = "px", pointsize = 20, bg = "white", res = NA)
plot(bottom$Position,bottom$Mean.Cov,type="l",xlab="Position relative to TSS",ylab="Read depth",col="green",ylim=c(0,1.2))
polygon(c(bottom$Position,rev(bottom$Position)),c(bottom$LowerBound,rev(bottom$UpperBound)),col=rgb(1,0,0,0.2), border = FALSE)
lines(bottom$Position,bottom$Mean.Cov,col=rgb(1,0,0))
polygon(c(top$Position,rev(top$Position)),c(top$LowerBound,rev(top$UpperBound)),col=rgb(0.75,0.2,1,0.2), border = FALSE)
lines(top$Position,top$Mean.Cov,col=rgb(0.75,0.2,1))
dev.off()

top<-read.csv("MergedControls_Plasma_FPKM_over8_tss.txt",header=TRUE,sep="\t")
bottom<-read.csv("MergedControls_Plasma_FPKM_between1_8_tss.txt",header=TRUE,sep="\t")
top100<-read.csv("MergedControls_Plasma_FPKM_between0.1_1_tss.txt",header=TRUE,sep="\t")
low<-read.csv("MergedControls_Plasma_FPKM_under0.1_tss.txt",header=TRUE,sep="\t")
png(filename = paste("MergedControls_Plasma_FPKM_8_vs_1to8_vs_01to1_tss.png",sep=""), width = 1180, height = 1180, units = "px", pointsize = 20, bg = "white", res = NA)
